{% comment %}
  Main Page Section
  Displays main content for standard pages
{% endcomment %}

<div class="page-section">
  <div class="container">
    <div class="page-wrapper">
      <h1>{{ page.title }}</h1>
      
      <div class="page-content">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Main Page",
  "settings": []
}
{% endschema %}
